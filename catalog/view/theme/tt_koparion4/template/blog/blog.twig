{{ header }}<div class="container">    <ul class="breadcrumb">        {% for breadcrumb in breadcrumbs %}            <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>        {% endfor %}    </ul></div>  <div class="container">        <div class="main">  <div class="row">{{ column_left }}      {% if column_left and column_right %}          {% set class = 'col-sm-6' %}      {% elseif column_left or column_right %}          {% set class = 'col-sm-9' %}      {% else %}          {% set class = 'col-sm-12' %}      {% endif %}      <div id="content" class="{{ class }}">{{ content_top }}          <div class="row" style="margin-bottom: 20px">              <div class="col-md-3">                <h1>{{ heading_title }}</h1>              </div>              <div class="col-md-9" style="margin-top: 25px;">                  <div class="col-md-6">                      <div class="filter-group">                          <label style="margin-right: -5%; margin-top: 5px;" class="col-sm-5 control-label"                                 for="filter-blog-week">{{ select_filter_by_week }}</label>                          <div class="col-sm-7">                              <select class="form-control" name="filter-blog" id="filter-blog-week">                                  <option value="" selected>{{ text_no_filter_date }}</option>                                  <option value="1" >{{ option_1_week }}</option>                                  <option value="2" >{{ option_2_week }}</option>                                  <option value="3" >{{ option_3_week }}</option>                                  <option value="4" >{{ option_4_week }}</option>                              </select>                          </div>                      </div>                  </div>                  <div class="col-md-6">                      <div class="filter-group">                          <label style="margin-right: -5%; margin-top: 5px" class="col-sm-4 control-label"                                 for="filter-blog-status">{{ select_filter_by_status }}</label>                          <div class="col-sm-8">                              <select class="form-control" name="filter-blog" id="filter-blog-status">                                  <option value="" selected>{{ text_all_status }}</option>                                  <option value="UC">{{ text_up_coming }}</option>                                  <option value="OG">{{ text_on_going }}</option>                                  <option value="F">{{ text_finish }}</option>                              </select>                          </div>                      </div>                  </div>              </div>          </div>      {% if articles %}          <div class="article-page">            {% set i = 0 %}            {% for article in articles %}              {% set i = i + 1 %}                <div class="card col-md-3">                    <img class="card-img-top" src="{{ article.image }}" alt="Card image cap">                    {% if article.event_status == "UC" %}                    <div class="event-status up-coming-event">{{ text_up_coming }}</div>                    {% elseif article.event_status == "OG" %}                    <div class="event-status on-going-event">{{ text_on_going }}</div>                    {% elseif article.event_status == "F" %}                    <div class="event-status finish-event">{{ text_finish }}</div>                    {% endif %}                    <div class="card-body card-custom-body">                        <div class="card-title">{{ article.event_start_time }} - {{ article.event_start_time }}{% if article.author %}{{ " | " ~ article.author }}{% endif %}</div>                        <a class="card-text" href="{{ article.href }}">{{ article.name }}</a>                        <div style="padding: 10px 0 10px;">{{ article.intro_text }}</div>                        <div class="button-bottom">                            {% if article.event_link %}                            <a href="{{ article.event_link }}" class="btn_buy_ticket">{{ button_buy_ticket }}</a>                            {% endif %}                            <a class="btn-readmore readmore-page" style="float: right;" href="{{ article.href }}">{{ button_read_more }}</a>                        </div>                    </div>                </div>            {% endfor %}          </div>          <div class="toolbar toolbar-products toolbar-bottom">				<div class="toolbar-amount"><span>{{ results }}</span></div>				<div class="pages">{{ pagination }}</div>			</div>          <input type="hidden" id="blog_url" value="{{ blog_url }}">      {% else %}      <input type="hidden" id="blog_url" value="{{ blog_url }}">      <p>{{ text_no_event }}</p>      {% endif %}          {{ content_bottom }}</div>      {{ column_right }}</div></div></div>{{ footer }}<style>    .btn-readmore{        margin-right: 20%;    }    .card{        margin-bottom: 20px;    }    .card .readmore-page{        padding-bottom: 20px;    }    .card .card-custom-body{        height: 250px;        max-height: 250px;        padding: 10px 10px 25px 10px;        border: 1px solid rgb(224,224,224);        border-bottom-left-radius: 15px;        border-bottom-right-radius: 15px;    }    .card .card-custom-body .button-bottom{        width: 100%;        position: absolute;        bottom: 0;    }    .card .card-custom-body .card-text{        font-weight: 600;        font-size: 21px;    }    .card .card-custom-body .btn_buy_ticket{        -webkit-border-radius: 10px;        -moz-border-radius: 10px;        border-radius: 10px;        border: 1px solid #006f91;        background-color: #006f91;        color: white;        width: 100%;        height: 100%;        padding: 5px 10px;    }    .card .card-custom-body .card-title{        font-weight: 600;        text-transform: uppercase;    }    .card .card-img-top{        border-top-right-radius: 15px;        border-top-left-radius: 15px;    }    .event-status{        position: absolute;        top: 0;        right: 0;        margin: 10px 20px 0 0;    }    .event-status.up-coming-event{        background-color: #00abe0;        border-color: #00abe0;        padding: 5px 10px;        color: white;    }    .event-status.on-going-event{        background-color: #f07c29;        border-color: #f07c29;        padding: 5px 10px;        color: white;    }    .event-status.finish-event{        background-color: #666666;        border-color: #666666;        padding: 5px 10px;        color: white;    }    .card-img-top{        position: relative;    }    @media (max-width: 1200px){        .card .card-custom-body .card-text{            font-size: 18px;        }    }    @media (max-width: 992px) {        .card .card-custom-body .button-bottom{            margin-left: 5%;            width: 50%;        }        .btn-readmore{            margin-right: 15%;        }        .card .card-custom-body {            width: 370px;            margin-left: auto;            margin-right: auto;        }        .card {            text-align: center;        }        .card .card-custom-body .btn_buy_ticket {            margin-left: -60%;        }        .event-status {            margin: 10px 25% 0 0;        }    }    @media (max-width: 676px) {        .event-status {            margin: 10px 22% 0 0;        }        .card .card-custom-body .button-bottom {            width: 65%;        }        .btn-readmore{            margin-right: 15%;        }    }    @media (max-width: 574px) {        .event-status {            margin: 10px 18% 0 0;        }        .card .card-custom-body .button-bottom {            width: 65%;        }        .btn-readmore{            margin-right: 10%;        }    }    @media (max-width: 499px) {        .card .card-custom-body .button-bottom {            width: 80%;        }        .event-status {            margin: 10px 15% 0 0;        }        .btn-readmore{            margin-right: unset;        }        .card .card-custom-body {            width: auto;            margin-left: auto;            margin-right: auto;        }    }</style><script>    $(document).ready(function() {        var url = new URL(window.location.href);        var event_status = url.searchParams.get("event_status");        var week = url.searchParams.get("week");        if(event_status){            $('#filter-blog-status').val(event_status);        }else{            $('#filter-blog-status').val('');        }        if(week){            $('#filter-blog-week').val(week);        }else{            $('#filter-blog-week').val('');        }        // var max_height = 0;        // $('.card-custom-body').each( function(){        //     max_height = $(this).height() > max_height ? $(this).height() : max_height;        // })        //        // $('.card-custom-body').each( function(){        //     $(this).css('height', max_height);        // })        $('#filter-blog-week').on('change', function(){            if($(this).val()){                if (event_status){                    window.location = $('#blog_url').val() + '&week=' + $(this).val() +'&event_status=' + event_status;                } else{                    window.location = $('#blog_url').val() + '&week=' + $(this).val();                }            }else{                if(event_status){                    window.location = $('#blog_url').val() + '&event_status=' + event_status;                }else{                    window.location = $('#blog_url').val();                }            }        })        $('#filter-blog-status').on('change', function(){            if($(this).val()){                if(week){                    window.location = $('#blog_url').val() + '&event_status=' + $(this).val() + '&week=' + week;                }else{                    window.location = $('#blog_url').val() + '&event_status=' + $(this).val();                }            }else{                if(week){                    window.location = $('#blog_url').val() + '&week=' + week;                }else{                    window.location = $('#blog_url').val();                }            }        })    });</script>